[core]
	editor = vim
	autocrlf = input
[color]
	ui = true
[alias]
	st = status
	srb = svn rebase
	rb = rebase
	ci = commit
	sci = svn dcommit
	co = checkout
	di = diff
	br = branch
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
[apply]
	whitespace = fix
