command term urxvt
command lock 'xautolock -locknow'
command c "dclock -bg black -led_off black -date '%Y-%b-%d %a' -fontName 'Go Mono' -bw 0"

ignore dclock
autogroup 0 Dclock

sticky yes

bind-key M-F1	group-only-1
bind-key M-F2	group-only-2
bind-key M-F3	group-only-3
bind-key M-F4	group-only-4
bind-key M-F5	group-only-5
bind-key M-F6	group-only-6
bind-key M-F7	group-only-7
bind-key M-F8	group-only-8
bind-key M-F9	group-only-9
bind-key MS-F1	window-movetogroup-1
bind-key MS-F2	window-movetogroup-2
bind-key MS-F3	window-movetogroup-3
bind-key MS-F4	window-movetogroup-4
bind-key MS-F5	window-movetogroup-5
bind-key MS-F6	window-movetogroup-6
bind-key MS-F7	window-movetogroup-7
bind-key MS-F8	window-movetogroup-8
bind-key MS-F9	window-movetogroup-9
# conflict with kakoune
unbind-key M-h
unbind-key M-j
unbind-key M-k
unbind-key M-l
# conflict with elvish
unbind-key M-period
bind-key M-Return menu-ssh
